<UserControl x:Class="Shell.DigitalClock.DigitalClockDots"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    MinWidth="7" MinHeight="20" Padding="2">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Ellipse Grid.Row="1" Fill="AliceBlue" Name="p0" />
        <Ellipse Grid.Row="3" Fill="AliceBlue" Name="p1" />
    </Grid>
</UserControl>
